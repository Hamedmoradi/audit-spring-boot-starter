{
  "groups": [
    {
      "name": "audit",
      "type": "auditLogger.autoconfigure.SpringLoggingAutoConfiguration",
      "sourceType": "auditLogger.autoconfigure.SpringLoggingAutoConfiguration"
    }
  ],
  "properties": [
    {
      "name": "audit.servers",
      "defaultValue": "localhost:9092"
    },
    {
      "name": "audit.advertised.host.name",
      "defaultValue": "localhost"
    },
    {
      "name": "audit.listeners",
      "defaultValue": "SASL_PLAINTEXT://localhost:9090"
    },
    {
      "name": "audit.delete.topic.enable",
      "defaultValue": "true"
    },
    {
      "name": "audit.security.inter.broker.protocol",
      "defaultValue": "SAPLAINTEXT"
    },
    {
      "name": "audit.sasl.mechanism.inter.broker.protocol",
      "defaultValue": "PLAIN"
    },
    {
      "name": "audit.sasl.enabled.mechanisms",
      "defaultValue": "PLAIN"
    },
    {
      "name": "audit.advertised.listeners",
      "defaultValue": "SASL_PLAINTEXT://localhost:9090"
    },
    {
      "name": "audit.spring.kafka.producer.retries",
      "defaultValue": "0"
    },
    {
      "name": "audit.spring.kafka.producer.batch-size",
      "defaultValue": "100000"
    },
    {
      "name": "audit.spring.kafka.producer.request.timeout.ms",
      "defaultValue": "3000"
    },
    {
      "name": "audit.spring.kafka.producer.request.autostartup",
      "defaultValue": "false"
    },
    {
      "name": "audit.spring.kafka.producer.linger.ms",
      "defaultValue": "1"
    },
    {
      "name": "audit.spring.kafka.producer.acks",
      "defaultValue": "0"
    },
    {
      "name": "audit.spring.kafka.producer.buffer-memory",
      "defaultValue": "33554432"
    },
    {
      "name": "audit.spring.kafka.producer.max.block.ms",
      "defaultValue": "5000"
    },
    {
      "name": "audit.max.block.ms",
      "defaultValue": "6000"
    },
    {
      "name": "audit.spring.kafka.bootstrap-servers",
      "defaultValue": "localhost:9092,localhost:29092"
    },
    {
      "name": "audit.spring.kafka.producer.key-serializer",
      "type": "org.apache.kafka.common.serialization.StringSerializer",
      "sourceType": "org.apache.kafka.common.serialization.StringSerializer"
    },
    {
      "name": "audit.spring.kafka.producer.value-serializer",
      "type": "org.apache.kafka.common.serialization.StringSerializer",
      "sourceType": "org.apache.kafka.common.serialization.StringSerializer"
    }
  ]
}